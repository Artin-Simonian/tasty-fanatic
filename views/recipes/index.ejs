<title>Recipes</title>
<%- include('../partials/header') %>
<table id="list">
  <thead>
    <tr>
      <th>Recipe Name</th>
      <th>Recipe Details</th>
    </tr>
  </thead>
  <tbody>
    <% recipes.forEach(function(m) { %>
    <tr>
      <td><%= m.name %></td>
      <td><a href="/recipes/<%= m._id %>">Recipe</a></td>
    </tr>
    <% }); %>
  </tbody>
</table>

<%- include('../partials/footer') %>
